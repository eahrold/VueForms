{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 versions", "safari >= 7"],
        "uglify": true
      },
      "modules": "umd"
    }]
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-vue-jsx",
    ["transform-runtime", {
      "polyfill": true,
      "regenerator": true
    }]
  ]
}
